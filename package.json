{
  "name": "fpl-api",
  "version": "2.2.0",
  "description": "A wrapper for Fantasy Premier League API",
  "author": "Jeppe Smith <mail@jeppesmith.dk>",
  "keywords": [
    "premier league",
    "fantasy premier league",
    "fantasy",
    "fpl"
  ],
  "license": "MIT",
  "main": "./node/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeppe-smith/fpl-api.git"
  },
  "scripts": {
    "prebuild": "rimraf browser node",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "node post-build.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:watch": "nodemon",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/index.js",
    "graphql": "node graphql-definitions.js",
    "graphql:watch": "node graphql-definitions.js --watch",
    "validate": "tsc --noEmit -p tsconfig.build.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "document": "documentation readme src/node --parse-extension=ts --section=API --document-exported",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.2",
    "@types/tough-cookie": "^2.3.5",
    "fetch-cookie": "^0.7.3",
    "form-data": "^2.5.1",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.2",
    "tough-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "dotenv": "^8.2.0",
    "jest": "24",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1"
    },
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
