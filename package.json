{
  "name": "fpl-api",
  "version": "1.0.4",
  "description": "A wrapper for Fantasy Premier League API",
  "author": "Jeppe Smith <mail@jeppesmith.dk>",
  "keywords": [
    "premier league",
    "fantasy premier league",
    "fantasy",
    "fpl"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeppe-smith/fpl-api.git"
  },
  "scripts": {
    "prebuild": "node prebuild.js",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:watch": "nodemon",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/index.js",
    "graphql": "node graphql-definitions.js",
    "graphql:watch": "node graphql-definitions.js --watch",
    "validate": "tsc --noEmit",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "document": "documentation readme src --parse-extension=ts --section=API --document-exported",
    "prepublishOnly": "rimraf dist && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.5.2",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1",
    "typescript": "^3.6.4"
  },
  "typings": "./dist/index.d.ts",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^utils/(.*)$": "<rootDir>../utils/$1"
    },
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "src": "dist"
  },
  "optionalDependencies": {
    "node-fetch": "^2.6.0"
  }
}
